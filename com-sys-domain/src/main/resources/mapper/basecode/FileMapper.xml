<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sys.domain.basecode.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sys.model.basecode.File">
        <result column="id" property="id"/>
        <result column="file_name" property="fileName"/>
        <result column="file_type" property="fileType"/>
        <result column="file_size" property="fileSize"/>
        <result column="file_path" property="filePath"/>
        <result column="material_id" property="materialId"/>
        <result column="base64_type" property="base64Type"/>
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="colums_condition">
        id,
    file_name, file_type, file_size, file_path,material_id,base64_type
    </sql>

    <sql id="condition">
        <if test="null!=params">
            <if test="null!=params.queryCondition and ''!=params.queryCondition">
                And ${params.queryCondition}
            </if>
            <if test="params.id != null and params.id != ''">
                and id = #{params.id}
            </if>
            <if test="params.fileName != null and params.fileName != ''">
                and file_name like '%${params.fileName}'
            </if>
            <if test="params.fileType != null and params.fileType != ''">
                and file_type like '%${params.fileType}'
            </if>
            <if test="params.fileSize != null and params.fileSize != ''">
                and file_size like '%${params.fileSize}'
            </if>
            <if test="params.filePath != null and params.filePath != ''">
                and file_path like '%${params.filePath}'
            </if>
            <if test="params.materialId != null and params.materialId != ''">
                and material_id = #{params.materialId}
            </if>
        </if>
    </sql>

    <sql id="uniqe_condition">
        1=1
        <if test="null!=id and ''!=id">
            AND id=#{id}
        </if>
    </sql>

    <select id="findByPrimaryKey" resultMap="BaseResultMap">
        SELECT
        <include refid="colums_condition"/>
        FROM
        t_file
        WHERE id = #{id};
    </select>

    <select id="findByUnique" resultMap="BaseResultMap">
        SELECT
        <include refid="colums_condition"/>
        FROM t_file
        WHERE
        <include refid="uniqe_condition"/>
        limit 1
    </select>

    <select id="findByParam" resultMap="BaseResultMap" parameterType="map">
        SELECT
        <include refid="colums_condition"/>
        FROM t_file
        WHERE 1=1
        <include refid="condition"/>
        LIMIT #{page.startRowNum} ,#{page.pageSize}
    </select>

    <select id="selectCount" resultType="java.lang.Integer">
        SELECT COUNT(1) as s FROM t_file WHERE 1=1
        <include refid="condition"/>
    </select>

    <select id="selectByPage" resultMap="BaseResultMap" parameterType="map">
        SELECT
        <include refid="colums_condition"/>
        FROM t_file WHERE 1=1
        <include refid="condition"/>
        <if test="orderby != null and ''!=orderby">
            ORDER BY ${orderby}
        </if>
        LIMIT #{page.startRowNum} ,#{page.pageSize}
    </select>

    <select id="selectByParams" resultMap="BaseResultMap" parameterType="map">
        SELECT
        <include refid="colums_condition"/>
        FROM t_file t
        where 1=1
        <include refid="condition"/>
    </select>

    <delete id="deleteByPrimaryKey">
		DELETE FROM t_file
		WHERE id = #{id}
	</delete>

    <delete id="deleteByUnique">
        DELETE FROM t_file
        WHERE
        <include refid="uniqe_condition"/>
    </delete>

    <delete id="deleteByParams" parameterType="map">
        <if test="ids != null">
            DELETE
            FROM t_file t where t.id in
            <foreach item="item" index="index" collection="ids" open="(" separator="," close=")">
                #{item}
            </foreach>
        </if>
    </delete>

    <insert id="insert" parameterType="com.sys.model.basecode.File">
        INSERT INTO t_file
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="fileName != null">
                file_name,
            </if>
            <if test="fileType != null">
                file_type,
            </if>
            <if test="fileSize != null">
                file_size,
            </if>
            <if test="filePath != null">
                file_path,
            </if>
            <if test="materialId != null">
                material_id,
            </if>
            <if test="base64Type != null">
                base64_type,
            </if>

        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="fileName != null">
                #{fileName},
            </if>
            <if test="fileType != null">
                #{fileType},
            </if>
            <if test="fileSize != null">
                #{fileSize},
            </if>
            <if test="filePath != null">
                #{filePath},
            </if>
            <if test="materialId != null">
                #{materialId},
            </if>
            <if test="base64Type != null">
                #{base64Type},
            </if>
        </trim>
    </insert>

    <update id="update" parameterType="com.sys.model.basecode.File">
        UPDATE t_file
        <set>
            <if test="fileName != null">
                file_name = #{fileName},
            </if>
            <if test="fileType != null">
                file_type = #{fileType},
            </if>
            <if test="fileSize != null">
                file_size = #{fileSize},
            </if>
            <if test="filePath != null">
                file_path = #{filePath},
            </if>
            <if test="materialId != null">
                material_id = #{materialId},
            </if>
            <if test="base64Type != null">
                base64_type = #{base64Type},
            </if>
        </set>
        WHERE id = #{id}
    </update>
    <!-- auto generate end -->
</mapper>
